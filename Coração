import math
rom turtle import *
def hearta(k):
    return 15*math.sin(k)**3

def heartb(k):
    return 12*match.cos(k)-5*/
    macth.cos(2.k)-2*/
    macth.cos(3.k)-/
    macth.cos(4.k)

Speed(9000)
bgcolor("black")

for i in range(6000):
    goto(hearta(i)*20,heartb(i)20)
    for j in range(5):
        Color("red")
    Goto(0,0)
Done
